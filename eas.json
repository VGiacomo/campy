{
  "cli": {
    "version": ">= 12.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "debug": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_ANDROID_COMPILE_SDK_VERSION": "30",
          "EXPO_ANDROID_BUILD_TOOLS_VERSION": "30.0.3",
          "EXPO_ANDROID_MIN_SDK_VERSION": "21",
          "EXPO_ANDROID_TARGET_SDK_VERSION": "30"
        }
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
